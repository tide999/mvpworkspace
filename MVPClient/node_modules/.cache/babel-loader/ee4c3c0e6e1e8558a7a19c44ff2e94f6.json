CAAA,CAAA,EAAA,CAAA;AACA;AACA;AACA;;AACA,MAAA,QAAA,CAAA,cAAA,CAAA,EAAA,EAAA,KAAA,CAAA,OAAA,GAAA,MAAA;;AACA,UAAA,EAAA,IAAA,SAAA,EAAA;AACA,QAAA,QAAA,CAAA,sBAAA,CAAA,aAAA,EAAA,CAAA,EAAA,KAAA,CAAA,MAAA,GAAA,MAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,OAAA,EAAA,KAAA,CAAA,mBAAA,GAAA,MAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,WAAA,EAAA,KAAA,CAAA,oBAAA,GAAA,MAAA;AACA;;AACA,UAAA,KAAA,GAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,WAAA,EAAA,KAAA,CAAA,mBAAA,GAAA,MAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,WAAA,EAAA,KAAA,CAAA,oBAAA,GAAA,MAAA;AACA,OAnBA,CAoBA;AACA;AACA;AACA;;AACA,KAhCA;AAiCA,IAAA,WAjCA,uBAiCA,GAjCA,EAiCA;AACA,MAAA,QAAA,CAAA,cAAA,CAAA,GAAA,EAAA,KAAA,CAAA,OAAA,GAAA,OAAA;;AACA,UAAA,GAAA,IAAA,SAAA,EAAA;AACA,QAAA,QAAA,CAAA,sBAAA,CAAA,aAAA,EAAA,CAAA,EAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,OAAA,EAAA,KAAA,CAAA,mBAAA,GAAA,KAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,WAAA,EAAA,KAAA,CAAA,oBAAA,GAAA,KAAA;AACA,OANA,CAOA;AACA;AACA;AACA;;;AACA,WAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,YAAA,KAAA,GAAA,CAAA,CAAA,KAAA,GAAA,EAAA;AACA,eAAA,GAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA;AACA;;AAEA,UAAA,KAAA,GAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,WAAA,EAAA,KAAA,CAAA,mBAAA,GAAA,KAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,WAAA,EAAA,KAAA,CAAA,oBAAA,GAAA,KAAA;AACA;AACA;AAtDA;AAVA,CAAA","sourcesContent":["<template>\n    <div id=\"app\">\n        <div class=\"little-tips-content\">\n            <transition name=\"\" enter-active-class=\"animate__animated animate__backInDown\" leave-active-class=\"animate__animated animate__backOutUp\">\n                <div class=\"tips-item\" @click=\"showContent('mission')\" v-show=\"checkEleArr('mission')\">任务</div>\n            </transition>\n            <transition name=\"\" enter-active-class=\"animate__animated animate__backInDown\" leave-active-class=\"animate__animated animate__backOutUp\">\n                <div class=\"tips-item\" @click=\"showContent('winch')\" v-show=\"checkEleArr('winch')\">绞车</div>\n            </transition>\n            <transition name=\"\" enter-active-class=\"animate__animated animate__backInDown\" leave-active-class=\"animate__animated animate__backOutUp\">\n                <div class=\"tips-item\" @click=\"showContent('apparatus')\" v-show=\"checkEleArr('apparatus')\">仪器</div>\n            </transition>\n        </div>\n        <!-- <transition name=\"\" enter-active-class=\"animate__animated animate__bounceInDown\" leave-active-class=\"animate__animated animate__bounceOutUp\"> -->\n        <Mission @sendEleName=\"getMsg\" />\n        <!-- </transition> -->\n        <div class=\"btm-content\">\n            <Winch @sendEleName=\"getMsg\" />\n            <Apparatus @sendEleName=\"getMsg\" />\n        </div>\n    </div>\n</template>\n\n<script>\nimport Winch from './components/winch.vue'\nimport Mission from './components/mission.vue'\nimport Apparatus from './components/apparatus.vue'\n\nexport default {\n    name: 'App',\n    data() {\n        return {\n            ele: [],\n        }\n    },\n    components: {\n        Winch, Mission, Apparatus\n    },\n    methods: {\n        checkEleArr(name) {\n            if (this.ele.indexOf(name) == -1) {\n                return false\n            } else {\n                return true\n            }\n        },\n        getMsg(ev) {\n            if (this.ele.indexOf(ev) == -1) {\n                this.ele.push(ev);\n            } else {\n                for (var i in this.ele) {\n                    if (this.ele[i] == ev) {\n                        this.ele.splice(i, 1);\n                    }\n                }\n            }\n            document.getElementById(ev).style.display = 'none';\n            if (ev == 'mission') {\n                document.getElementsByClassName('btm-content')[0].style.height = '100%';\n                document.getElementById('winch').style.borderTopLeftRadius = '10px';\n                document.getElementById('apparatus').style.borderTopRightRadius = '10px';\n            }\n            if (this.ele.length == 2) {\n                document.getElementById('apparatus').style.borderTopLeftRadius = '10px';\n                document.getElementById('apparatus').style.borderTopRightRadius = '10px';\n            }\n            // if (ev == 'winch' && this.ele.indexOf('mission') >= 0) {\n            //     document.getElementById('apparatus').style.width = '100%'\n            //     document.getElementById('apparatus').style.flex = '0'\n            // }\n        },\n        showContent(ele) {\n            document.getElementById(ele).style.display = 'block';\n            if (ele == 'mission') {\n                document.getElementsByClassName('btm-content')[0].style.height = '77%'\n                document.getElementById('winch').style.borderTopLeftRadius = '0px';\n                document.getElementById('apparatus').style.borderTopRightRadius = '0px';\n            }\n            // if (ele == 'winch' && this.ele.indexOf('mission') >= 0) {\n            //     document.getElementById('apparatus').style.width = '100%'\n            //     document.getElementById('apparatus').style.flex = '1'\n            // }\n            for (var i in this.ele) {\n                if (this.ele[i] == ele) {\n                    this.ele.splice(i, 1);\n                }\n            }\n\n            if (this.ele.length < 2) {\n                document.getElementById('apparatus').style.borderTopLeftRadius = '0px';\n                document.getElementById('apparatus').style.borderTopRightRadius = '0px';\n            }\n        }\n    }\n}\n</script>\n\n<style>\n#app {\n    font-family: Avenir, Helvetica, Arial, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    color: #2c3e50;\n    background: #606266;\n    width: 100%;\n    height: 100%;\n    overflow: auto;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n}\n.btm-content {\n    /* height: auto; */\n    width: 99%;\n    height: 77%;\n    display: flex;\n    margin-top: 8px;\n}\n#mission {\n    /* border-radius: 10px; */\n    border-top-left-radius: 10px;\n    border-top-right-radius: 10px;\n    width: 99%;\n    height: 22%;\n    background: #fff;\n    box-shadow: 1px 3px 3px #f60;\n    margin-top: 8px;\n    /* overflow: auto; */\n}\n#winch {\n    /* border-radius: 10px; */\n    border-bottom-left-radius: 10px;\n    width: 200px;\n    height: 98%;\n    background: #fff;\n    box-shadow: 1px 3px 3px #f60;\n    margin-right: 8px;\n    overflow: auto;\n}\n#apparatus {\n    /* border-radius: 10px; */\n    border-bottom-right-radius: 10px;\n    flex: 1;\n    height: 98%;\n    background: #fff;\n    box-shadow: 1px 3px 3px #f60;\n    overflow: auto;\n}\n.el-input__inner {\n    padding: 0;\n}\n.little-tips-content {\n    position: fixed;\n    top: 0;\n    right: 100px;\n    display: flex;\n    z-index: 999999;\n}\n.little-tips-content .tips-item {\n    background: #fff;\n    height: 30px;\n    line-height: 30px;\n    margin: 0 5px 0 5px;\n    padding: 0 10px;\n    border-bottom-right-radius: 10px;\n    border-bottom-left-radius: 10px;\n    border: 1px solid #fff;\n    box-shadow: 0px 3px 3px #888;\n    cursor: pointer;\n}\n</style>\n"],"sourceRoot":"src"}]}