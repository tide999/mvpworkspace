"mini\" class=\"from-content\" inline :label-position=\"labelPosition\">\n                    <el-form-item label=\"数据时间\">\n                        <el-input v-model=\"form.getDataTime\" style=\"width:150px;\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"仪器下降速度(m/s)\">\n                        <el-input v-model=\"form.instrumentDownSpeed\" style=\"width:120px;\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"仪器深度(m)\">\n                        <el-input v-model=\"form.instrumentDepth\" style=\"width:80px;\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"温度(℃)\">\n                        <el-input v-model=\"form.temperature\" style=\"width:80px;\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"电导率(S/m)\">\n                        <el-input v-model=\"form.electricConductivity\" style=\"width:90px;\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"压力(bar)\">\n                        <el-input v-model=\"form.pressure\" style=\"width:65px;\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"盐度(psu)\">\n                        <el-input v-model=\"form.salinity\" style=\"width:70px;\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"声速(m/s)\">\n                        <el-input v-model=\"form.soundVelocity\" style=\"width:70px;\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"PH()\">\n                        <el-input v-model=\"form.ph\" style=\"width:85px;\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"浊度(ug/l)\">\n                        <el-input v-model=\"form.turbidity\" style=\"width:100px;\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"叶绿素(ug/l)\">\n                        <el-input v-model=\"form.chlorophyll\" style=\"width:100px;\"></el-input>\n                    </el-form-item>\n                </el-form>\n            </div>\n            <div id=\"myChart\" :style=\"{ width: '100%', height: '450px' }\"></div>\n            <div class=\"apparatus-from-content-btm\">\n                <el-form ref=\"form\" :model=\"formdown\" size=\"mini\" class=\"from-content\" inline>\n                    <el-form-item label=\"当前海深(米)\">\n                        <el-input v-model=\"formdown.oceanDeep\" style=\"width:80px\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"经度\">\n                        <el-input v-model=\"formdown.longitude\" style=\"width:120px\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"纬度\">\n                        <el-input v-model=\"formdown.latitude\" style=\"width:120px\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"航速(节)\">\n                        <el-input v-model=\"formdown.speed\" style=\"width:100px\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"数据保存位置\">\n                        <el-input v-model=\"formdown.dataFileAddress\" style=\"width:250px\"></el-input>\n                    </el-form-item>\n                </el-form>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: 'apparatus',\n    data() {\n        return {\n            labelPosition: \"top\",\n            labelPosition1: \"left\",\n            form: {\n                getDataTime: '2020-09-10 15:13:45',\n                instrumentDownSpeed: '-27.16286',\n                instrumentDepth: '90.92881',\n                temperature: '18.769869',\n                electricConductivity: '0.01287323',\n                pressure: '60.93201',\n                salinity: '9.082812',\n                soundVelocity: '1233.870',\n                ph: '1.81818273',\n                turbidity: '0.62736273',\n                chlorophyll: '0.0000002736',\n            },\n            formdown: {\n                oceanDeep: '221.2389212',\n                longitude: '29.239102',\n                latitude: '213.23182',\n                speed: '0',\n                dataFileAddress: `D:/Datas`,\n            },\n            myChart: \"\",\n            option: {\n                title: {\n                    text: ''\n                },\n                tooltip: {\n                    trigger: 'axis'\n                },\n                legend: {\n                    data: ['深度(米)', '当前海深(米)', '温度(℃)', '导电率(S/m)', '压力(bar)', '盐度(psu)', '声速(m/s)', 'PH()', '浊度(ug/l)', '叶绿素(ug/l)']\n                },\n                grid: {\n                    left: '3%',\n                    right: '4%',\n                    bottom: '3%',\n                    containLabel: true\n                },\n                xAxis: {\n                    type: 'category',\n                    boundaryGap: false,\n                    data: ['0', '20', '40', '60', '80', '100'],\n                    axisLine: { show: true },\n                    axisTick: { show: true },\n                    splitLine: { show: true },\n                },\n                yAxis: {\n                    type: 'value',\n                    axisLine: { show: true },\n                    axisTick: { show: true },\n                    splitLine: { show: true },\n                },\n                series: [\n                    {\n                        name: '深度(米)',\n                        type: 'line',\n                        stack: '总量',\n                        data: [120, 132, 101, 134, 90, 230]\n                    },\n                    {\n                        name: '当前海深(米)',\n                        type: 'line',\n                        stack: '总量',\n                        data: [220, 182, 191, 234, 290, 330]\n                    },\n                    {\n                        name: '温度(℃)',\n                        type: 'line',\n                        stack: '总量',\n                        data: [150, 232, 201, 154, 190, 330]\n                    },\n                    {\n                        name: '导电率(S/m)',\n                        type: 'line',\n                        stack: '总量',\n                        data: [320, 332, 301, 334, 390, 330]\n                    },\n                    {\n                        name: '压力(bar)',\n                        type: 'line',\n                        stack: '总量',\n                        data: [820, 932, 901, 934, 1290, 1330]\n                    },\n                    {\n                        name: '盐度(psu)',\n                        type: 'line',\n                        stack: '总量',\n                        data: [820, 932, 901, 934, 1290, 1330]\n                    },\n                    {\n                        name: '声速(m/s)',\n                        type: 'line',\n                        stack: '总量',\n                        data: [820, 932, 901, 934, 1290, 1330]\n                    },\n                    {\n                        name: 'PH()',\n                        type: 'line',\n                        stack: '总量',\n                        data: [820, 932, 901, 934, 1290, 1330]\n                    },\n                    {\n                        name: '浊度(ug/l)',\n                        type: 'line',\n                        stack: '总量',\n                        data: [820, 932, 901, 934, 1290, 1330]\n                    },\n                    {\n                        name: '叶绿素(ug/l)',\n                        type: 'line',\n                        stack: '总量',\n                        data: [820, 932, 901, 934, 1290, 1330]\n                    }\n                ]\n            }\n        }\n    },\n    beforeMount() {\n    },\n    mounted() {\n\n        this.drawLine();\n        let _this = this;\n        window.onresize = function () {\n            _this.myChart.resize()\n        }\n    },\n    methods: {\n        drawLine() {\n            let _this = this;\n            // 基于准备好的dom，初始化echarts实例\n            this.myChart = this.$echarts.init(document.getElementById(\"myChart\"));\n            this.myChart.setOption(this.option, true);\n            let elementResizeDetectorMaker = require(\"element-resize-detector\");//引入监听dom变化的组件\n            let erd = elementResizeDetectorMaker();\n            let worldMapContainer = document.getElementById('apparatus');\n            erd.listenTo(worldMapContainer, function (element) {  //执行监听 \n                _this.$nextTick(function () {\n                    document.getElementById(\"myChart\").style.width = element.offsetWidth\n                    _this.myChart.resize(); //变化重新渲染饼图\n                    \n                })\n            })\n        },\n        onSubmit() {\n            console.log('submit!');\n        },\n        closeItem(ele) {\n            this.$emit('sendEleName', ele);\n        },\n    }\n}\n</script>\n\n<style scoped>\n.apparatus-content{\n    min-width: 1200px;\n}\n.apparatus-from-content {\n    margin: 8px 0 0 12px;\n}\n.apparatus-from-content .from-content {\n    display: flex;\n    flex-wrap: nowrap;\n    justify-content: space-between;\n}\n.apparatus-from-content-btm {\n    margin: 10px 0 0 12px;\n}\n.apparatus-from-content-btm .from-content {\n    display: flex;\n    flex-wrap: nowrap;\n    justify-content: space-between;\n}\n.page-title {\n    font-size: 16px;\n    color: #409eff;\n    position: relative;\n    margin: 5px 10px;\n    line-height: 32px;\n    border-bottom: 1px solid #eee;\n    cursor: default;\n}\n.page-title .close-btn {\n    position: absolute;\n    right: 0px;\n    top: 0px;\n    cursor: pointer;\n    color: #303133;\n}\n</style>"]}]}